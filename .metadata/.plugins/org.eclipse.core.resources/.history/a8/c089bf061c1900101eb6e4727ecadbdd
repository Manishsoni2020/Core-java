package oops_constructor;

class Calculator 
{
	public Calculator()
	{
		System.out.println("welcome to my calculator");
	}
	public Calculator(int i,int j)
	{
		int c=i+j;
		System.out.println("sum of no. is :"+c);
	}
	public Calculator(int i,int o)
	{
		
	}
}

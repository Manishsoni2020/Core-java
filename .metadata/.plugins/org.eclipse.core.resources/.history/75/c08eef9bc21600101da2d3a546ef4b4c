package flow_control;

public class For_each_loop {
	public static void main(String[] args)
	{
//		1.Print element of array
//		int a[]= {11,12,13,14,15};
//		for(int n:a)
//		{
//			System.out.print(n+" ");
//		}
		
//		2.Even and Odd count
		int[] arr= {12, 7, 5, 20, 33, 10, 2};
		int evencount=0;
		int oddcount=0;
		for(int a:arr)
		{
			if(a%2==0)
			{
				evencount++;
			}
			else 
			{
				oddcount++;
			}
		}
	}
}

package collections_class;

import java.util.*;
public class List_searching {
public static void main(String[] args) {
	ArrayList al=new ArrayList();
	al.add(40);
	al.add(20);
	al.add(55);
	al.add(5);
	al.add(100);
	System.out.println(al);
	Collections.sort(al);
	System.out.println(al);
	System.out.println(Collections.binarySearch(al, 55));
}
}
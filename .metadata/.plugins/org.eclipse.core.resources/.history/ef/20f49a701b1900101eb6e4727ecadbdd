package oops_constructor;

class Library 
{
	int book_id;
	String writer;
	
	Library(int book_id,String writer)
	{
		this.book_id=book_id;
		this.writer=writer;
	}
	
	static void display()
	{
		System.out.println("Book id is "+book_id);
		
	}
}
public class Parameterized_constructor 
{
	public static void main(String[]args)
	{
		
	}
}

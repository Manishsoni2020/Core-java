package Interface_method;

interface left 
{
	public void m1(); //same name method 
}
interface right
{
	public void m1(int i); //same name method
}
class A implements left,right
{
	public void m1() //in this case we can only implement one method
	{
		System.out.println("Hello Developer");
	}
	
	public void m1(int i)
	{
		System.out.println("the no is "+i);
	}
}
public class Interface_method_case2 {
	public static void main(String[]args)
	{
		Test obj=new Test();
		obj.show();
	}
}

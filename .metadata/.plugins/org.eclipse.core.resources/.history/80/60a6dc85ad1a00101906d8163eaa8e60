package multithreading;

class Display
{
	public synchronized void wish(String name)
	{
		for(int i=1;i<=10;i++)
		{
			System.out.println("Good morning");
			try
			{
				Thread.sleep(3000);
			}
			catch(Exception e)
			{
				
			}
			System.out.println(name);
		}
	}
}

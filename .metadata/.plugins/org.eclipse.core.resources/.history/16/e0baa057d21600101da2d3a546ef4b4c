package declaration_and_accessmodifiers;

class P
{
	public void property()
	{
		System.out.println("I am property class");
	}
	public final void marry()
	{
		System.out.println("I am final class");
	}
}
class C extends P
{
	public void marry()
	{
		System.out.println("I am normal marry class");
	}
}
public class Final {
	public static void main(String[]args)
	{
		
	}
}
